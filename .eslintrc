{
  "root": true,
  "parser": "@babel/eslint-parser",
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "quotes": ["error", "single"],
    "prettier/prettier": "warn",
    "react/prop-types": "off",
    "react/state-in-constructor": ["error", "never"],
    "react/display-name": ["off"]
  }
}
